#!/bin/bash
# mcedit TAB: 4 with spaces

### BEGIN HEADER
name=$(basename ${0})
spname="__SP_${name}"
default_kickstart="${HOME}/shellpack/lib/kickstart"
if test -z "${SHELLPACK_KICKSTART}" ; then
  if test -r "${default_kickstart}" ; then
    SHELLPACK_KICKSTART="${default_kickstart}"
  else
    echo "kickstart error"
    exit 10
  fi
fi
# read kickstart
. ${SHELLPACK_KICKSTART}
# load libraries
uselib ssh
### END HEADER


### BEGIN MAIN
_SSH_AGENT_PID=`ps ax | grep ssh-agent | awk '/\?\?/{print $1}'`
if test "${_SSH_AGENT_PID}" != "" ; then
  echo "Killing ssh-agent"
  kill ${_SSH_AGENT_PID}
fi
### END MAIN
